{
  "description": "Maps motor vehicle type variations to Connective CRM dropdown values (7 vehicle types)",
  "mappings": {
    "Bike": ["Bike", "Motorcycle", "Motorbike", "Bicycle", "Two-Wheeler"],
    "Large": ["Large", "Large Vehicle", "Truck", "Van"],
    "Luxury Car": ["Luxury Car", "Luxury", "Premium Car", "High-End Car", "Prestige Car"],
    "4WD": ["4WD", "Four Wheel Drive", "4x4", "SUV", "Off-Road"],
    "Medium": ["Medium", "Medium Car", "Sedan", "Mid-Size"],
    "Small": ["Small", "Small Car", "Compact", "Hatchback"],
    "Small Medium": ["Small Medium", "Small-Medium", "Small to Medium", "Intermediate"]
  },
  "fallback_strategy": "fuzzy_match",
  "fuzzy_match_threshold": 80,
  "case_sensitive": false,
  "notes": {
    "conditional_field": "Only appears when Asset Type = Motor Vehicle",
    "parent_field": "Asset Type must be Motor Vehicle first"
  }
}
