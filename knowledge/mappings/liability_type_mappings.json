    "Car Loan": ["Car Loan", "Vehicle Loan", "Auto Loan", "Car Finance", "Vehicle Finance"],
    "Commercial Bill": ["Commercial Bill", "Comm Bill", "Business Bill", "Trade Bill"],
    "Credit Card": ["Credit Card", "CC", "Visa", "Mastercard", "Amex", "Card"],
    "HECS": ["HECS", "HECS-HELP", "HELP", "Student Debt", "Student Loan", "Education Debt"],
    "Hire Purchase": ["Hire Purchase", "HP", "Hire Purch"],
    "Lease": ["Lease", "Equipment Lease", "Vehicle Lease", "Car Lease"],
    "Line of Credit": ["Line of Credit", "Line Of Credit", "LOC", "Credit Line", "Line Credit"],
    "Loan as Guarantor": ["Loan as Guarantor", "Loan As Guarantor", "Guarantor Loan", "Guarantee Loan"],
    "Mortgage": ["Mortgage", "Home Loan", "Property Loan", "Mortgage Loan", "Housing Loan"],
    "Loan ATO": ["Loan ATO", "ATO Loan", "Tax Office Loan", "ATO Debt", "ATO Payment Plan"],
